{
  "name": "datarose/laravel-macros",
  "description": "Set of useful Laravel macros",
  "keywords": [
    "datarose",
    "laravel-macros"
  ],
  "homepage": "https://github.com/datarose/laravel-macros",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rózsa Zoltán",
      "homepage": "https://github.com/rozsazoltan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "amphp/parallel": "^0.2.5",
    "amphp/parallel-functions": "^0.1.3",
    "mockery/mockery": "^1.4.2",
    "orchestra/testbench": "^6.23|^7.0",
    "phpunit/phpunit": "^9.4.4",
    "spatie/laravel-ray": "^1.29",
    "symfony/stopwatch": "^5.2|^6.0"
  },
  "autoload": {
    "psr-4": {
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
