{
  "name": "datarose/illuminate-macros",
  "description": "Set of useful Laravel macros",
  "keywords": [
    "datarose",
    "laravel framework",
    "illuminate-macros"
  ],
  "homepage": "https://github.com/datarose/illuminate-macros",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rózsa Zoltán",
      "homepage": "https://github.com/rozsazoltan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Datarose\\": "src/Illuminate/Foundation",
      "IlluminateMacro\\": "src/Illuminate/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IlluminateMacro\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Datarose\\IlluminateMacroServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
