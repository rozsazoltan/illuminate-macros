{
  "name": "datarose/laravel-macros",
  "description": "Set of useful Laravel macros",
  "keywords": [
    "datarose",
    "laravel-macros"
  ],
  "homepage": "https://github.com/datarose/laravel-macros",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Rózsa Zoltán",
      "homepage": "https://github.com/rozsazoltan",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^9.0",
    "illuminate/support": "^9.0"
  },
  "autoload": {
    "psr-4": {
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
